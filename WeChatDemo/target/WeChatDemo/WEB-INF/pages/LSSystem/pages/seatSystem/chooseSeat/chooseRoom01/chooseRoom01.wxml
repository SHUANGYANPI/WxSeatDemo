<view class="title" >{{floor}}</view>

<view class="room">
    <view class="room01" id="{{room01}}" bindtap="getRoomId">
        <navigator url="../../room/room0101/room0101" open-type="navigate">
            <view class="top01"><cover-image src="../../../image/room01.png"></cover-image></view>
            <view class="buttom01"><text >{{room01}}\n{{empty}}/{{all}}</text></view>
        </navigator>
    </view>
    <view class="room02" id="{{room02}}" bindtap="getRoomId">
        <navigator url="../../room/room0102/room0102" open-type="navigate">
            <view class="top02"><cover-image src="../../../image/room02.png"></cover-image></view>
            <view class="buttom02"><text >{{room02}}\n{{empty}}/{{all}}</text></view>
        </navigator>
    </view>
</view>
    <view class="room03"  id="{{room03}}" hidden="{{hiddenRoom}}" bindtap="getRoomId">
        <navigator url="../../room/room0102/room0102" open-type="navigate">
            <view class="top03"><cover-image src="../../../image/room03.png"></cover-image></view>
            <view class="buttom03"><text>{{room03}}\n{{empty}}/{{all}}</text></view>
        </navigator>
    </view>
