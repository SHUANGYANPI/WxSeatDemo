page{
  background: #F0F8ff;
}
swiper{
  width: 100%;
  height: 220rpx;
}
swiper-item{
  width:100%;
}
.indexbanner-image{
  width: 100%;
  height: 100%;
}
.image{
  width: 100%;
  height: 100%;
}


view{
  text-align: center;
}
.room{
  
  background-color: rgb(0, 238, 255);
}

.table{
  border: 1px solid white;
}
.top{
  display: flex;
  border: 1px solid white;
  width: 100%;
  justify-content: center;
  background: #FFFFFF;
  align-items: center;
  height: 4rem;
}
.topLeft{
  font-style: italic;
  
}
.topRight{
  margin-left: 10rpx;
  width: 12%;
  height: auto;
}

.one_bgc,.botn_bgc{
  margin-top: 35rpx;
  display:flex;
  width:100%;
  height: 5rem;
  justify-content: center;
  align-items: center;
  background: #FFFFFF;
}
.two_bgc{
  display:flex;
  width:100%;
  height: 5rem;
  justify-content: center;
  align-items: center;
  background: #FFFFFF;
}
.one1,.one2,.two1,.two2{
  margin-left: 35rpx;
  margin-right: 35rpx;
  display:flex;
  width:30%;
  height: 4rem;
  flex-direction:column;
  background: #FFFFFF;
}
.oneTop,.twoTop{
  display: flex;
  width: 100%;
  height: 65%;
  justify-content: center; 
  align-items: center;
}
.oneBotton,.twoBotton{
  display: flex;
  width: 100%;
  height: 25%;
  
  justify-content: center; 
  align-items: center;
}


.botton{
  margin-top: 45rpx;
  display: flex;
  width: 100%;
  height: 5rem;
  flex-direction:column;
  justify-content: center;
  align-items: center;
  background: #FFFFFF;

}
.bottonTop{
  display: flex;
  width: 40%;
  height: 65%;
  justify-content: center; 
  align-items: center;
}
.bottonBotton{
  display: flex;
  width: 100%;
  height: 35%;
  
  justify-content: center; 
  align-items: flex-start;
}

.btColor{
  opacity: 0.9;
  transform: scale(0.85,0.85);
  background: #7FFFD4;
}